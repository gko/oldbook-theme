    },
    "tokenColors": [
        {
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#867a67",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["punctuation"],
            "settings": {
                "foreground": "#d6c9b2"
            }
        },
        {
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#b08a5a",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function",
                "entity.name.class",
                "entity.name.type",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#a6b0b8"
            }
        },
        {
            "scope": [
                "string",
                "constant.numeric",
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#c6b88a"
            }
        },
        {
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#b08a5a"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#a6b0b8"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#b87e7e"
            }
        }
    ]
}
